{
	"editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "search.exclude": {
    "**/*.code-search": false,
    "**/bower_components": false,
    "**/node_modules": false
  },
  "editor.tokenColorCustomizations": null,
  "files.eol": "\n",
  "cSpell.words": [
    "polymersheet"
  ],
	"[rust]": {
		"editor.defaultFormatter": "statiolake.vscode-rustfmt"
},
}
